<?xml version="1.0" encoding="UTF-8" ?>
<ruleset name="TEST">
    <rule>
        <rule name="Not same judge on same side">
            <!-- if the judge has seen the team on the current side, they cannot be assigned to the team -->
            <subjects>
                <subject type="TEAM" name="team"/>
                <subject type="JUDGE" name="judge"/>
                <subject type="ROUND" name="match">
                    <match>
                        <history subject="team"/>
                        <history subject="judge"/>
                    </match>
                </subject>
            </subjects>
            <action value="NOT">
                <EXISTS subject="round"/>

            </action>

        </rule>
    </rule>
    <method>

    </method>
</ruleset>
